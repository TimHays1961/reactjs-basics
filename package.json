{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Basic react.js ",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && robocopy src dist index.html && webpack-dev-server -d --content-base src/ --inline --hot",
    "build:prod": "webpack -p && robocopy src dist index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Tim Hays",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
